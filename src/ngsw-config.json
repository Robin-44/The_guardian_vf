{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "resources": {
        "files": [
          "/assets/**",
          "/manifest.webmanifest",
          "/custom-service-worker.js" 
        ]
      }
    }
  ]
}
